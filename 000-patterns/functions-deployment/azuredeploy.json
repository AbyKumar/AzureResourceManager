{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "notConfigured": {
      "type": "string",
      "defaultValue": "notConfigured",
      "allowedValues": [ "notConfigured" ],
      "metadata": { "description": "This parameter is used for all expression that have not been configured yet." }
    },
    "String": {
      "type": "string",
      "defaultValue": "String",
      "minLength": 1,
      "maxLength": 24
    },
    "Int": {
      "type": "int",
      "defaultValue": 10,
      "minValue": 1,
      "maxValue": 1000
    },
    "Array": {
      "type": "array",
      "defaultValue": [ "this", "is", "an", "array" ]
    },
    "Boolean": {
      "type": "bool",
      "defaultValue": true,
      "allowedValues": [ true, false ]
    },
    "Object": {
      "type": "object",
      "defaultValue": {
        "node1": {
          "sub1a": "value1a",
          "sub1b": "value1b"
        },
        "node2": {
          "sub2a": "value2a",
          "sub2b": "value2b"
        }
      }
    },
    "SecureString": {
      "type": "securestring",
      "defaultValue": "PassW0rd"
    }
  },
  "variables": {
      "deployment": "[deployment()]"
  },
  "resources": [ ],
  "outputs": {

    "deployment": {
      "type": "object",
      "value": "[variables('deployment')]"
    }
  }
}
